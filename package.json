    "@jovotech/server-lambda": "^4.5.3",
    "@jovotech/target-serverless": "^4.1.8",
    "@nlpjs/lang-de": "^4.23.4",
    "@nlpjs/lang-en": "^4.23.4",
    "cors": "^2.8.5",
    "jexl": "^2.3.0",
    "jovo-v4-community-hook-magic-prototyper": "^0.0.42",
    "jovo-v4-community-hook-model-linter": "^0.0.2",
    "jovo-v4-community-plugin-jexl-output": "^0.0.16",
